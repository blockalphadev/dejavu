{
    "name": "@dejavu/application",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts"
        },
        "./common": {
            "types": "./src/common/index.ts",
            "import": "./src/common/index.ts"
        },
        "./market": {
            "types": "./src/market/index.ts",
            "import": "./src/market/index.ts"
        },
        "./user": {
            "types": "./src/user/index.ts",
            "import": "./src/user/index.ts"
        },
        "./order": {
            "types": "./src/order/index.ts",
            "import": "./src/order/index.ts"
        }
    },
    "scripts": {
        "typecheck": "tsc --noEmit",
        "lint": "eslint src/",
        "build": "tsc",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@dejavu/shared": "workspace:*",
        "@dejavu/domain": "workspace:*",
        "@dejavu/events": "workspace:*"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "typescript": "^5.4.0",
        "vitest": "^2.0.0"
    }
}